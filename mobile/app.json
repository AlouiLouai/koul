{
  "expo": {
    "name": "Makelti",
    "slug": "makelti-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "makelti",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": {
        "light": "./assets/ios-light.png",
        "dark": "./assets/ios-dark.png"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.makelti.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundImage": "./assets/adaptive-background.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.makelti.app",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Bach takhtar taswira men galerie mta3ek! üñºÔ∏è",
          "cameraPermission": "Bach nsawrou sa7nek w n9ouloulek chnoua fih! üì∏"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-icon-light.png",
          "backgroundColor": "#f1f5f9",
          "imageWidth": 280,
          "dark": {
            "backgroundColor": "#0f172a",
            "image": "./assets/splash-icon-dark.png"
          }
        }
      ],
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "c73de80d-2bb3-4422-af5a-10b7efb60ee7"
      }
    },
    "owner": "mnm89"
  }
}
{
  "expo": {
    "name": "Makelti",
    "slug": "makelti-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "makelti",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.makelti.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.makelti.app"
    },
    "extra": {
      "eas": {
        "projectId": "1baef3a5-ee2e-4bb0-a67e-794489f4c258"
      }
    },
    "plugins": [
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to let you analyze your food from an image.",
          "cameraPermission": "The app needs access to your camera to let you analyze your food from a photo."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-icon.png",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#141626"
          }
        }
      ]
    ],
    "owner": "mnm89"
  }
}